{
  "schemaVersion": "3.0.0",
  "files": {
    "reference": "reference.fasta",
    "pathogenJson": "pathogen.json",
    "genomeAnnotation": "annotation.gff3",
    "treeJson": "tree.json",
    "changelog": "CHANGELOG.md",
    "readme": "README.md",
    "examples": "sequences.fasta"
  },
  "compatibility": {
    "cli": "3.0.0-alpha.0",
    "web": "3.0.0-alpha.0"
  },
  "shortcuts": ["measles"],
  "defaultCds": "N",
  "attributes": {
    "name": "Measles morbillivirus -- complete genome",
    "reference name": "Ichinose-B95a",
    "reference accession": "NC_001498 "
  },
  "alignmentParams":{
    "retryReverseComplement": true
  },
  "qc": {
    "missingData": {
      "enabled": true,
      "missingDataThreshold": 2000,
      "scoreBias": 500,
      "scoreWeight": 50
    },
    "snpClusters": {
      "enabled": true,
      "windowSize": 100,
      "clusterCutOff": 6,
      "scoreWeight": 50
    },
    "mixedSites": {
      "enabled": true,
      "mixedSitesThreshold": 15,
      "scoreWeight": 50
    },
    "frameShifts": {
      "enabled": true,
      "scoreWeight": 20
    },
    "stopCodons": {
      "enabled": true,
      "scoreWeight": 50
    }
  }
}
